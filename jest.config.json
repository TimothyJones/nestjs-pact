{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/"],
  "coverageDirectory": "./coverage",
  "coverageReporters": [
    "text",
    "cobertura"
  ],
  "testEnvironment": "node"
}
